<%= form_with(model: @room, local: true) do |form| %>
  <div class="name">
    <%= form.label :物件名 %>
    <%= form.text_field :name %>
  </div>
  <div class="rent">
    <%= form.label :賃料 %>
    <%= form.text_field :rent %>
  </div>
  <div class="address">
    <%= form.label :住所 %>
    <%= form.text_field :address %>
  </div>
  <div class="age">
    <%= form.label :築年数 %>
    <%= form.text_field :age %>
  </div>
  <div class="note">
    <%= form.label :備考 %>
    <%= form.text_field :note %>
  </div>
  <div class="actions">
  <%= form.submit %>
  </div>


<% end %>